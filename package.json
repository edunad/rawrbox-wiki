{
    "name": "rawrbox",
    "packageManager": "yarn@3.6.3",
    "version": "0.0.0",
    "private": true,
    "main": "./index.js",
    "scripts": {
        "start": "docusaurus start --no-open",
        "serve": "docusaurus serve --dir .bin --no-open",
        "build": "yarn build-lib && docusaurus build --out-dir .bin",
        "build-lib": "node . --out docs/rawrbox/lua_api",
        "swizzle": "docusaurus swizzle"
    },
    "dependencies": {
        "@docusaurus/core": "2.4.3",
        "@docusaurus/preset-classic": "2.4.3",
        "@easyops-cn/docusaurus-search-local": "^0.36.0",
        "@mdx-js/react": "^1.6.22",
        "clsx": "^2.0.0",
        "prism-react-renderer": "^1.3.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "remark-git-contributors": "^4.0.1",
        "remark-github": "^12.0.0"
    },
    "devDependencies": {
        "@docusaurus/module-type-aliases": "2.4.3",
        "@edunad/lua-wiki-generator": "1.0.9",
        "@tsconfig/docusaurus": "^2.0.1",
        "docusaurus-plugin-sass": "^0.2.5",
        "prettier": "^3.0.3",
        "sass": "^1.68.0",
        "typescript": "^5.2.2",
        "yargs": "^17.7.1"
    },
    "browserslist": {
        "production": [
            ">0.5%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "engines": {
        "node": ">=16.14"
    }
